<%- include('./partials/header', {title: 'Students'}) %>



    <h1>STUDENT DATA</h1>

   
    <a class="btn btn-primary btn-lg" href="/" role="button">Back</a>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">id</th>
          <th scope="col">First_name</th>
          <th scope="col">Last_name</th>
          <th scope="col">Email</th>
          <th scope="col">Gender</th>
          <th scope="col">Birth_date</th>
          <th scope="col">Delete</th>
          <th scope="col">Edit</th>
        </tr>
      </thead>
      <tbody>
      <% for(var i = 0; i < data.length; i++){ %>
        <tr>
          <td><%= data[i].id%></td>
          <td><%= data[i].first_name %></td>
          <td><%= data[i].last_name %></td>
          <td><%= data[i].email %></td>
          <td><%= data[i].gender %></td>
          <td><%= data[i].birth_date%></td>
          <td><a href="/students/<%= data[i].id %>/delete">Delete</a></td>
          <td><a href="/students/<%=data[i].id %>/edit">Edit</a></td>
        </tr>

      <% } %>
      </tbody>
    </table>
 <%- include ('./partials/footer') %>