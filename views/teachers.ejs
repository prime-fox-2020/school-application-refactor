<%- include('./partials/top')%>
  </nav>
  <div class="jumbotron">
    <% if (data.length) { %>
    <table class="table table-striped">
    <thead>
      <tr>
        <th scope="col">ID</th>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
        <th scope="col">Email</th>
        <th scope="col">Gender</th>
      </tr>
    </thead>
    <tbody>
      <% data.forEach(el => { %>
        <tr>
          <th scope="row"><%= el.id %></th>
          <td><%= el.first_name %></td>
          <td><%= el.last_name %></td>
          <td><%= el.email %></td>
          <td><%= el.gender %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
  <% } %>
  </div>
</body>
</html>