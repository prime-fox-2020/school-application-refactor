    <%- include('../partials/header') %>
    <div class="container">
      <h3>Students Data</h3>

      <table class="highlight">
        <thead>
          <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Birth Of Date</th>
            <th>Email</th>
            <th>Option</th>
          </tr>
        </thead>

        <tbody>
          <% data.forEach(s => { %>
          <tr>
            <td><%= s.id %></td>
            <td><%= s.first_name %></td>
            <td><%= s.last_name %></td>
            <td><%= s.dbToIndonesian() %></td>
            <td><%= s.email %></td>
            <td><a href="/students/<%= s.id %>/edit">Edit</a> | <a href="/students/<%= s.id %>/delete" onclick="return confirm('Are you sure you want to delete this item?')">Delete</a></td>
          </tr>
          <% }) %>
        </tbody>
      </table>
      <br>
      <a href="/students/add" class="waves-effect waves-light btn"><i class="material-icons right">add</i>Add Student</a>
      <br>
      <p></p>
    </div>
    <%- include('../partials/footer') %>